{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.0",
  "description": "Repo Template: webpack, mocha-chai, babel, css, eslint",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 120",
    "ChromeAndroid >= 120",
    "Edge >= 120",
    "Firefox >= 121",
    "FirefoxAndroid >= 122",
    "Safari >= 17.1",
    "iOS >= 17.1",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "clean": "rimraf dist/*",
    "dev": "npm run build-dev",
    "build": "npm run build-prod",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "start": "webpack serve --open --mode development",
    "preview": "webpack serve --open --mode production",
    "test": "mocha",
    "watch": "webpack --watch --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "babel-loader": "^9.1.3",
    "chai": "^5.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.36.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "mocha": "^10.3.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "engines": {
    "node": "^18.19.0 || >=20.11.0"
  }
}
