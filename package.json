{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.0.0",
  "description": "Repo Template: webpack, mocha-chai, babel, css, eslint",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 112",
    "ChromeAndroid >= 112",
    "Edge >= 112",
    "Firefox >= 112",
    "FirefoxAndroid >= 112",
    "Safari >= 16.4",
    "iOS >= 16.4",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "clean": "rimraf dist/*",
    "dev": "npm run build-dev",
    "build": "npm run build-prod",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "start": "webpack serve --open --mode development",
    "preview": "webpack serve --open --mode production",
    "test": "mocha",
    "watch": "webpack --watch --mode development"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.6",
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.10",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.34.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "html-webpack-plugin": "^5.5.4",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
